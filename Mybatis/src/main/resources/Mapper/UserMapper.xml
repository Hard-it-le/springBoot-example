<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yjl.mybatis.mapper.UserMapper">


    <select id="selectUser" resultType="com.yjl.mybatis.entity.User">
        select username, password, enabled
        from users;
    </select>
</mapper>